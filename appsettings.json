{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsSettings": {
    "AllowOrigins": [ "http://localhost:3000" ],
    "AllowMethods": [ "GET", "POST", "PUT", "DELETE" ],
    "AllowHeaders": [ "Content-Type", "Authorization" ]
  },
  "Kestrel": {
    "Endpoints": {
      "MyHttpEndpoint": {
        "Url": "http://localhost:8080"
      }
    }
  }
}
